        "import pandas as pd\n",
        "from sklearn.linear_model import LinearRegression\n",
        "df = pd.read_csv(\"immovables.csv\")\n",
        "lr = LinearRegression()\n",
        "y = df[\"賃料\"]\n",
        "x = df.drop(\"賃料\", axis=1)\n",
        "lr.fit(x, y)\n",
        "print(\"予測:\", lr.predict(x))\n",
        "print(\"実際:\", y.to_numpy())"
      ]
    }
