        "import pandas as pd\n",
        "from sklearn.neighbors import NearestNeighbors\n",
        "df = pd.read_csv(\"drink.csv\")\n",
        "pivot = df.pivot(index=\"飲み物名\",\n",
        "                 columns=\"ユーザーID\",\n",
        "                 values=\"評価値\")\n",
        "knn = NearestNeighbors()\n",
        "knn.fit(pivot)"
      ]
